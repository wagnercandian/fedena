<% form_for :holiday do |f| %>
<%= select :holiday, :weekdays, [ "Segunda", "Terça","Quinta","Quarta","Sexta","Sábado", "Domingo" ]%>

  <%= submit_tag "Enviar" %>
<% end %>
<% unless @weekly_holidays.nil? %>
  <% @weekly_holidays.each do |d| %>
    <%= d.config_value %><%= link_to "delete", :action=>"delete", :id=>d.id  %>
    
  <% end %>
<% end %>